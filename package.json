{
  "name": "@j-meira/mui-theme",
  "version": "1.0.3",
  "description": "UI theme made with MUI 5",
  "author": "J-Meira",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "theme",
    "material",
    "react"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@eslint/js": "^9.18.0",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-links": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@storybook/theming": "^8.4.7",
    "@testing-library/react": "^16.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.6",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "8.20.0",
    "@typescript-eslint/parser": "8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "9.18.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-react": "7.37.4",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-react-refresh": "0.4.18",
    "fs-extra": "^11.2.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.4.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sass": "^1.83.4",
    "storybook": "^8.4.7",
    "ts-jest": "^29.2.5",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.0.7"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.3.1",
    "@mui/x-date-pickers": "^7.23.6",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "notistack": "^3.0.1",
    "react-icons": "^5.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/J-Meira/mui-theme.git"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "pnpm pre-build && pnpm build:pkg && pnpm copy-scss",
    "build:pkg": "pnpm build:esm && pnpm build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --declarationDir dist/esm",
    "clean": "node clean.ts",
    "copy-scss": "node copy.ts",
    "pre-build": "pnpm prettier && pnpm lint  && pnpm clean",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "lint": "eslint .",
    "test": "jest --config jestconfig.json",
    "sb": "storybook dev -p 6006",
    "sb-ssl": "storybook dev -p 6006 -h dev.jm.app.br --https --ssl-ca ./.ssl/ca.crt --ssl-cert ./.ssl/cert.crt --ssl-key ./.ssl/cert.key",
    "build-sb": "storybook build",
    "up-version": "pnpm version --no-git-tag-version",
    "pre-release": "pnpm pre-build && pnpm up-version"
  }
}
