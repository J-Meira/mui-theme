  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "theme",
    "material",
    "react"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@storybook/addon-essentials": "7.0.20",
    "@storybook/addon-interactions": "7.0.20",
    "@storybook/addon-links": "7.0.20",
    "@storybook/blocks": "7.0.20",
    "@storybook/client-api": "^7.0.20",
    "@storybook/manager-api": "^7.0.20",
    "@storybook/react": "7.0.20",
    "@storybook/react-vite": "7.0.20",
    "@storybook/testing-library": "0.0.14-next.2",
    "@storybook/theming": "^7.0.20",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.1",
    "jest-environment-jsdom": "^29.5.0",
    "prettier": "^2.8.3",
    "prop-types": "15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.62.1",
    "storybook": "7.0.20",
    "ts-jest": "^29.1.0",
    "tslib": "^2.4.1",
    "typescript": "^4.4.2",
    "vite": "^4.3.9"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "@mui/styled-engine": "^5.11.0",
    "@mui/styled-engine-sc": "^5.11.0",
    "@mui/x-date-pickers": "^5.0.11",
    "formik": "^2.2.9",
    "moment": "^2.29.4",
    "react-toastify": "^9.1.1",
    "styled-components": "^5.3.6"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/J-Meira/mui-theme.git"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn copy-files",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc",
    "clean": "node clean.ts",
    "copy-files": "copyfiles -u 1 src/**/*.scss dist",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prepublishOnly": "yarn build",
    "prepare": "yarn prettier && yarn lint && yarn clean",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "storybook": "storybook dev -p 6006 -h dev.jm.app.br --https --ssl-ca ./.ssl/ca.crt --ssl-cert ./.ssl/cert.crt --ssl-key ./.ssl/cert.key",
    "build-storybook": "storybook build"
  }
}
