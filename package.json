{
  "name": "@j-meira/mui-theme",
  "version": "2.0.2",
  "description": "UI theme made with MUI 5",
  "author": "J-Meira",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "theme",
    "material",
    "react"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/js": "^9.39.1",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-links": "^10.1.2",
    "@storybook/addon-onboarding": "^10.1.2",
    "@storybook/react": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "8.46.2",
    "@typescript-eslint/parser": "8.46.2",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-storybook": "10.1.2",
    "fs-extra": "^11.3.2",
    "globals": "^16.5.0",
    "jest": "^30.2.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.7.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "sass": "^1.94.2",
    "storybook": "^10.1.2",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.5",
    "@mui/x-date-pickers": "^8.19.0",
    "dayjs": "^1.11.19",
    "formik": "^2.4.9",
    "notistack": "^3.0.2",
    "react-icons": "^5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/J-Meira/mui-theme.git"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "pnpm pre-build && pnpm build:pkg && pnpm copy-scss",
    "build:pkg": "pnpm build:esm && pnpm build:cjs",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --declarationDir dist/esm",
    "clean": "node clean.ts",
    "copy-scss": "node copy.ts",
    "pre-build": "pnpm prettier && pnpm lint  && pnpm clean",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "lint": "eslint .",
    "test": "jest --config jestconfig.json",
    "sb": "storybook dev -p 6006",
    "sb-ssl": "storybook dev -p 6006 -h dev.jm.app.br --https --ssl-ca ./.ssl/ca.crt --ssl-cert ./.ssl/cert.crt --ssl-key ./.ssl/cert.key",
    "build-sb": "storybook build",
    "up-version": "pnpm version --no-git-tag-version",
    "pre-release": "pnpm pre-build && pnpm up-version"
  }
}
