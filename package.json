{
  "name": "@j-meira/mui-theme",
  "version": "0.4.20",
  "description": "ui theme made with MUI 5",
  "author": "J-Meira",
  "license": "ISC",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "keywords": [
    "theme",
    "material",
    "react"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@storybook/addon-essentials": "^8.0.10",
    "@storybook/addon-interactions": "^8.0.10",
    "@storybook/addon-links": "^8.0.10",
    "@storybook/blocks": "^8.0.10",
    "@storybook/client-api": "^7.6.17",
    "@storybook/manager-api": "^8.0.10",
    "@storybook/react": "^8.0.10",
    "@storybook/react-vite": "^8.0.10",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "^8.0.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.10",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "jest": "^29.7.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sass": "^1.77.0",
    "storybook": "^8.0.10",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.16",
    "@mui/x-date-pickers": "^7.3.2",
    "dayjs": "^1.11.11",
    "formik": "^2.4.6",
    "notistack": "^3.0.1",
    "react-icons": "^5.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/J-Meira/mui-theme.git"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "yarn build:esm && yarn build:cjs && yarn copy-scss",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build:esm": "tsc --declarationDir dist/esm",
    "clean": "node clean.ts",
    "copy-scss": "node copy.ts",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "prepublishOnly": "yarn build",
    "prepare": "yarn prettier && yarn lint && yarn clean",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "storybook": "storybook dev -p 6006 -h dev.jm.app.br --https --ssl-ca ./.ssl/ca.crt --ssl-cert ./.ssl/cert.crt --ssl-key ./.ssl/cert.key",
    "build-storybook": "storybook build",
    "update-version": "yarn version --no-git-tag-version",
    "release": "yarn update-version && yarn prepare"
  }
}
